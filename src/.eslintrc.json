{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],

  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "react/prop-types": 1,
    "no-undef": 0,
    "indent": ["off"],
    "linebreak-style": ["error", "unix"],
    // "function-paren-newline": ["error", "always"],
    // "quotes": ["error", "double"],
    "semi": ["error", "always"],
    "no-unused-vars": "off",
    "no-console": "off"
  }
}
